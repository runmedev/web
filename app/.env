# Runtime app configuration should come from /configs/app-configs.yaml.
# __INITIAL_STATE__ bootstrap configuration is deprecated for new settings.
# VITE_* values in this file are fallback defaults only.

# Google OAuth credentials (required)
VITE_OAUTH_CLIENT_ID="notavalidclientid"
VITE_OAUTH_CLIENT_SECRET="notavalidclientsecret"

# Google OAuth configuration (default values, typically unchanged)
VITE_OAUTH_SCOPE="openid email profile"
VITE_OIDC_DISCOVERY_URL="https://accounts.google.com/.well-known/openid-configuration"
